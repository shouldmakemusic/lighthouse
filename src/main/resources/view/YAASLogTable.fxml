<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.collections.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.collections.FXCollections ?>

<AnchorPane prefHeight="369.0" prefWidth="352.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="net.hirschauer.yaas.lighthouse.visual.YaasLogController">
	<children>
		<BorderPane prefHeight="369.0" prefWidth="352.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<top>
				<HBox prefHeight="34.0" prefWidth="352.0" BorderPane.alignment="CENTER">
					<children>
						<ComboBox fx:id="levelCombobox" prefHeight="33.0"
							prefWidth="100.0" promptText="Debug">
							<items>
								<FXCollections fx:factory="observableArrayList">
									<String fx:value="Debug" />
									<String fx:value="Info" />
									<String fx:value="Warn" />
								</FXCollections>
							</items>
						</ComboBox>
					</children>
				</HBox>
			</top>
			<center>
				<AnchorPane>
					<children>
						<TableView fx:id="yaasLogEntryTable" prefHeight="400.0"
							prefWidth="353.0" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0" BorderPane.alignment="CENTER">
							<columns>
								<TableColumn fx:id="levelColumn" prefWidth="110.0"
									text="Level" />
								<TableColumn fx:id="messageColumn" prefWidth="241.0"
									text="Message" />
							</columns>
							<opaqueInsets>
								<Insets />
							</opaqueInsets>
						</TableView>
					</children>
				</AnchorPane>
			</center>
		</BorderPane>
	</children>
</AnchorPane>
